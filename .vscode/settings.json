{
  "editor.tabSize": 2,
  "files.exclude": {
    "src/main/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": false
  },
  "eslint.options": {
    "extensinons": [
      ".ts",
      ".js",
      ".tsx",
      ".jsx"
    ]
  },
  "javascript.validate.enable": true,
  "editor.quickSuggestions": {
    "strings": true,
  }
}